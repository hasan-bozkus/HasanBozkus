


<div class="row">
    <div class="col-lg-6">
        <div class="widget navy-bg no-padding">
            <div class="p-m">
                <h1 class="m-xs">DashBoard</h1>

                <h3 class="font-bold no-margins">
                    Bu sitede Asp.Net Core 5.0 Teknolosisi kullanıldı
                </h3>
                <small>
                    Burası Yöneticiler tarafından yönetilmektedir.
                </small>
            </div>
            <div class="flot-chart">
                <div class="flot-chart-content" id="flot-chart1"></div>
            </div>
        </div>
    </div>
    <div class="col-lg-3">
        <div class="widget lazur-bg no-padding">
            <div class="p-m">
                <h1 class="m-xs">Son Blog</h1>
                <h3 class="font-bold no-margins">
                    @ViewBag.v1
                </h3>
                <small>Tüm BlogLar</small>
            </div>
            <div class="flot-chart">
                <div class="flot-chart-content" id="flot-chart2"></div>
            </div>
        </div>
    </div>
    <div class="col-lg-3">
        <div class="widget yellw-bg no-padding">
            <div class="p-m">
                <h1 class="m-xs">$ 50,992</h1>

                <h3 class="font-bold no-margins">
                    Blog Site KAzancı
                </h3>
                <small>Ürün Tanıtımları,</small>
                <small>Promosyonlar,</small>
                <small>Reklamlar,</small>
                <small>Anlaşmalar.</small>
            </div>
            <div class="flot-chart">
                <div class="flot-chart-content" id="flot-chart3"></div>
            </div>
        </div>
    </div>
</div>